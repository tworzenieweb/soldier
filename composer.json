{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.0.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "jms/di-extra-bundle": "1.1.*",
        "friendsofsymfony/user-bundle": "dev-master",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/user-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "sonata-project/seo-bundle": "dev-master",
        "sonata-project/notification-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "dev-master",
        "knplabs/knp-gaufrette-bundle": "dev-master",
        "vich/uploader-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "dev-master",
        "sonata-project/page-bundle": "dev-master",
        "genemu/form-bundle": "2.1.x-dev",
        "sensio/generator-bundle": "2.1.x-dev",
        "sonata-project/news-bundle": "dev-master",
        "sonata-project/media-bundle": "dev-master",
        "sonata-project/formatter-bundle": "dev-master",
        "knplabs/knp-markdown-bundle": "1.1.1",
        "sonata-project/intl-bundle": "dev-master",
        "symfony-cmf/symfony-cmf": "1.0.x-dev",
        "jackalope/jackalope-jackrabbit": "1.0.*",
        "jackalope/jackalope-doctrine-dbal": "dev-master",
        "doctrine/phpcr-bundle": "1.0.*",
        "doctrine/phpcr-odm": "1.0.*",
        "symfony-cmf/simple-cms-bundle": "1.0.x-dev",
        "friendsofsymfony/rest-bundle": "dev-master",
        "symfony-cmf/create-bundle": "1.0.x-dev",
        "sonata-project/doctrine-phpcr-admin-bundle": "1.0.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
